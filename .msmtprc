# Set defaults.
defaults
# Enable or disable TLS/SSL encryption.
tls on
tls_starttls on

# Setup engelsystem account's settings gmail
account engelgmail
tls_certcheck off
auth login
host smtp.gmail.com
port 587
user <username>
from <username>@gmail.com
password <password>
logfile /var/log/msmtp/msmtp.log